{
  "name": "@haetae/javascript",
  "version": "0.0.13",
  "description": "Utilities like automatic dependency graph resolver of Javascript ecosystem (e.g. including Typescript, JSX) for Haetae",
  "license": "MIT",
  "private": false,
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "ts": "node --loader ts-node/esm --loader ../../loader.js",
    "lint": "eslint --ext .js,.cjs,.mjs,.jsx,.ts,.tsx,.md .",
    "lint:md": "markdownlint .",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage -c ../../jest.config.js ./packages/javascript/",
    "build": "tsc --build tsconfig.build.json",
    "prepublishOnly": "tsc --build tsconfig.build.json"
  },
  "peerDependencies": {
    "@haetae/core": "workspace:~",
    "@haetae/utils": "workspace:~"
  },
  "dependencies": {
    "@haetae/common": "workspace:~",
    "dependency-tree": "^8.1.1",
    "dirname-filename-esm": "^1.1.1",
    "find-up": "^6.3.0",
    "memoizee": "^0.4.15",
    "read-pkg-up": "^9.1.0",
    "serialize-javascript": "^6.0.0",
    "upath": "^2.0.1",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@haetae/core": "workspace:~",
    "@haetae/utils": "workspace:~"
  },
  "keywords": [
    "haetae",
    "haetae-javascript",
    "haetae/javascript",
    "haetaejavascript",
    "haetae-js",
    "haetae/js",
    "haetaejs",
    "javascript",
    "typescript",
